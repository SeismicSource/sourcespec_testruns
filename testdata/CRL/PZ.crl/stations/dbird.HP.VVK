  begin_channel
    "00" EHE VEL1 none DIG1 none 2010.07.01-10:00:00 present HP
    "00" EHN VEL2 none DIG1 none 2010.07.01-10:00:00 present HP
    "00" EHZ VEL3 none DIG1 none 2010.07.01-10:00:00 present HP
  end_channel
  
end_station

