begin_station
   #  verifier la profondeur du capteur
  VVK 2010.07.01-10:00:00 present 38.42235 21.8115 650.0 00.000 "Vomvokou, Nafpaktos, Greece"
  Owner "INSU"
  
  begin_location
    Location "00" 38.42235 21.8115 650.0 00.000 VVK
  end_location
  
  begin_sensor
